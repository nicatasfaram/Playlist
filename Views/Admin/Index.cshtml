@model IEnumerable<User>


@{
    ViewData["Title"] = "AdminController";
}

<h1>AdminController</h1>


<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            
            
            <td>
                <form asp-action="Delete" asp-route-id="@user.Id" method="post">
                    <button type="submit" class="btn btn-sm btn-danger">
                        Delete
                    </button>
                </form>
            </td>
        </tr>
    }
</table>

